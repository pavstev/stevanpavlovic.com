{
  "[css]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[html]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[json]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.formatOnSave": true,
  "typescript.experimental.useTsgo": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.suggest.autoImports": true,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    ".env": ".env.local, .env.production, .env.development, .env.test, .env.example",
    ".gitignore": ".gitattributes, .gitmodules",
    "*.config.ts": "${capture}.config.*.ts, ${capture}.config.mjs, ${capture}.config.cjs",
    "*.config.js": "${capture}.config.*.js",
    "*.json": "${capture}.jsonc, ${capture}.json5",
    "*.ts": "${capture}.js, ${capture}.mjs, ${capture}.cjs, ${capture}.test.ts, ${capture}.spec.ts, ${capture}.e2e.ts, ${capture}.d.ts, ${capture}.stories.ts",
    "*.tsx": "${capture}.jsx, ${capture}.test.tsx, ${capture}.spec.tsx, ${capture}.stories.tsx",
    "knip.config.ts": "knip.config.*.ts, knip.config.mjs, knip.config.cjs",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, .nvmrc, .npmrc, pnpm-workspace.yaml",
    "README.md": "CHANGELOG.md, LICENSE, CONTRIBUTING.md, CODE_OF_CONDUCT.md, SECURITY.md",
    "tsconfig.json": "tsconfig.*.json, jsconfig.json",
    "eslint.config.ts": "eslint.config.*.ts, eslint.config.mjs, eslint.config.cjs, knip.config.*",
    "astro.config.ts": "wrangler.json*"
  },
  "files.associations": {
    "*.html.template": "html",
    "*.ts.template": "typescript",
    "*.tsx.template": "typescriptreact",
    "wrangler.json": "jsonc"
  },
  "files.exclude": {
    "**/scripts/**/*": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "dist/": true,
    "node_modules/": true,
    ".cache/": true,
    ".vite/": true,
    "coverage/": true,
    ".nyc_output/": true,
    "playwright-report/": true,
    "test-results/": true,
    "src/routeTree.gen.ts": true,
    ".tanstack/": true
  },
  "search.exclude": {
    "**/worker-configuration.d.ts": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/.astro": true,
    "dist/": true,
    ".cache/": true,
    ".vite/": true,
    "coverage/": true,
    ".nyc_output/": true,
    "playwright-report/": true,
    "test-results/": true,
    "pnpm-lock.yaml": true,
    "src/routeTree.gen.ts": true,
    ".tanstack/": true
  },
  "tailwindCSS.includeLanguages": {
    "typescript": "javascript",
    "typescriptreact": "html"
  },
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["cx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"],
    ["clsx\\(([^)]*)\\)", "'([^']*)'"],
    ["clsx\\(([^)]*)\\)", "\"([^\"]*)\""],
    ["clsx\\(([^)]*)\\)", "`([^`]*)`"]
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "astro",
    "typescript",
    "typescriptreact"
  ],
  "emmet.includeLanguages": {
    "typescript": "html",
    "typescriptreact": "html"
  },
  "cSpell.words": ["Zoomable"],
  "typescript.tsdk": "node_modules/typescript/lib",
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode"
  }
}
