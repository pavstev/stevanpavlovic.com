---
import LayoutRoot from "../components/layout/layout-root.astro";
import Meteors from "../components/meteor.astro";
import Container from "../components/molecules/container.astro";
import HomeBento from "../components/organisms/home-bento.astro";
import { PROFILE } from "../config";
---

<LayoutRoot class="pb-10" description={PROFILE.bio} slug="/" title={`${PROFILE.name} | Software Engineer`}>
  <Container>
    <Meteors number={30} />

    <HomeBento />
  </Container>
</LayoutRoot>
